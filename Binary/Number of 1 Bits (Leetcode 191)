1. O(K)
class Solution {
public:
    int hammingWeight(int n) {
        int count = 0;
        for(int i=0; i<32; i++){
            if(n>>i & 1) count++;
        }
        return count;
    }
};

2, Brian Kernighan's Algorithm
class Solution {
public:
    int hammingWeight(int n) {
       int count = 0;
       while(n){
        n = n&(n-1);
        count++;
       }
       return count;
    }
};
